<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rent.mysdaapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="drawingapp.MainActivity"
            android:label="@string/draw_app_name"/>
        <activity
            android:name="simplegame.MainActivity"
            android:label="@string/game_app_name"/>
        <activity
            android:name="gallery.GalleryActivity"
            android:label="Drawing Gallery"/>
    </application>

</manifest>